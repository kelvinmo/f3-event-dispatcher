{
    "name": "kelvinmo/f3-event-dispatcher",
    "description": "A PSR-14 compliant event dispatcher for the Fat-Free Framework",
    "keywords": [ "event", "events" ],
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/event-dispatcher": "^1.0",
        "bcosca/fatfree-core": "v4.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.3",
        "phpstan/phpstan": "^1.3"
    },
    "provide": {
        "psr/event-dispatcher-implementation": "1.0"
    },
    "autoload": {
        "psr-4": { "F3\\EventDispatcher\\": "lib/"}
    },
    "autoload-dev": {
        "classmap": [ "tests/" ]
    },
    "scripts": {
        "test": [ "@composer install", "phpunit" ],
        "phpstan": [ "@composer install", "phpstan analyse" ]
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Kelvin Mo",
            "email": "kmo@users.sourceforge.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-features/f3-v4": "v2.0-dev"
        }
    }
}
